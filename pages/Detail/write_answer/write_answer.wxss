.wrap {
  height: 808rpx;
  background: white;
  padding: 29rpx 24rpx 0rpx 24rpx;
  margin-bottom: 24rpx;
  position: relative;
}

.wrap_center {
  padding-bottom: 35rpx;
  border-bottom: 1rpx solid #e9e9e9;
}

.wrap_center text:nth-child(1) {
  margin-right: 25rpx;
}

.wrap_center text:nth-child(2) {
  color: #333;
  font-size: 32rpx;
}

#textarea {
  width: 100%;
  height: 648rpx;
  font-size: 32rpx;
}

.img-v {
  margin-top: -200rpx;
  position: relative;
  z-index: 999;
}

