/* pages/my_page/collection/collection.wxss */
page {
  background: #f5f5f5;
}
.clear:after {
  content: '';
  width: 0;
  height: 0;
  display: block;
  clear: both;
  overflow: hidden;
}
.tit{
  width: 100%;
  height: 88rpx;
  background: #fff;
  display: flex;
  align-items: center;
  justify-content: center;
}
.tit>view{
  width: 20%;
  text-align: center;
  height: 88rpx;
}
.tit>view>text{
  border-bottom: 6rpx solid transparent;
  line-height: 82rpx;
  font-size: 28rpx;
  color: #666;
  display: inline-block;
}
.tit_clear{
  border-bottom: 6rpx solid #2577E3!important;
  color: #2577E3!important;
}

.content{
  width: 100%;
}
.boxs{
  width: 100%;
  background: #fff;
  margin-top: 24rpx;
}
.boxs>view:nth-child(1){
  font-size: 36rpx;
  color: #333;
  font-weight: bold;
  padding-top: 37rpx;
  padding-bottom: 37rpx;
  margin-left: 27rpx;
}
.boxs_content{
  display: flex;
  justify-content:space-between;
  align-items: center;
  /* width: 100%; */
  box-sizing: border-box;
  padding-left: 27rpx;
  padding-right: 24rpx;
  padding-bottom: 35rpx;
}
.boxs_content>text{
  font-size: 30rpx;
  color: #666;
}
.boxs_content>image{
  width: 550rpx;
  height: 128rpx;
  border-radius: 12rpx;
}
.boxs_bottom{
  box-sizing: border-box;
  width: 100%;
  padding-left: 24rpx;
  padding-bottom: 32rpx;
}
.boxs_bottom>view{
  float: left;
  font-size: 25rpx;
  color: #999;
}
.boxs_bottom>view:nth-child(1){
  margin-right: 47rpx;
}
.huida{
  font-size: 30rpx!important;
  color: #666!important;
  font-weight: 400!important;
  box-sizing: border-box;
  padding-right: 28rpx;
}
.huida_img_box{
  margin-left: 30rpx;
  margin-right: 30rpx;
  box-sizing: border-box;
  padding-bottom: 25rpx;
  border-bottom: 1rpx solid #e9e9e9;
}
.huida_img{
  width: 190rpx;
  height: 128rpx;
  border-radius: 12rpx;
  float: left;
  margin-left: 18rpx;
}
.huida_img:nth-child(1){
  margin-left: 0!important;
}
.huada_tit{
  font-size: 28rpx;
  color: #333;
  padding-left: 26rpx;
  font-weight: bold;
  line-height: 98rpx;
}
.zhuti_img{
  width: 190rpx!important;
}
.boxs_contents{
  padding-left: 0!important;
}
.left_zhuti{
  margin-top: 30rpx;
}
.left_zhuti>view{
  float: left;
  font-weight: 400!important;
  font-size: 24rpx!important;
  color: #999!important;
}
.zhuti_title{
  font-size: 32rpx;
  font-weight: bold;
  color: #333;
}






.sort_content{
  width: 100%;
  background: #fff;
  box-sizing: border-box;
  padding: 0 24rpx;
  margin-bottom: 25rpx;
}
.sort_content_tit{
  width: 100%;
  height: 70rpx;
  padding-top: 35rpx;
  display: flex;
  align-items: center;
  justify-content:space-between;
}
.sort_content_tit>image{
  width: 60rpx;
  height: 60rpx;
}
.sort_content_tit_left>image{
  width: 70rpx;
  height: 70rpx;
  border-radius: 35rpx;
  float: left;
}
.xinxi{
  float: left;
  display: flex;
  flex-direction: column;
  justify-content:center;
  margin-left: 17rpx;
}
.xinxi>view{
  line-height: 100%;
}
.sort_content_name{
  font-size: 28rpx;
  color: #2577E3;
