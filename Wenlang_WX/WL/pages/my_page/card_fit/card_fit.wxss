/* pages/my_page/card_fit/card_fit.wxss */
page {
  background: #f5f5f5;
}
.clear:after {
  content: '';
  width: 0;
  height: 0;
  display: block;
  clear: both;
  overflow: hidden;
}

.box{
  width: 100%;
  background: #fff;
  margin-top: 25rpx;
}
.list{
  width: 100%;
  height: 110rpx;
  box-sizing: border-box;
  display: flex;
  justify-content:space-between;
  align-items: center;
  padding: 0 25rpx;
  border-bottom: 1rpx solid #e9e9e9;
}
.list>text{
  font-size: 32rpx;
  color: #333;
}
.list>view{
  width: 134rpx;
  height: 60rpx;
  border-radius: 30rpx;
  text-align: center;
  line-height: 60rpx;
  color: #666;
  font-size: 26rpx;
  background: #f5f5f5;
}
.xianshi{
  color: #2577E3!important;
}
.baocun{
   width: 702rpx;
  height: 88rpx;
  border-radius: 44rpx;
  background: linear-gradient(to left, #2677E2 , #409AFF);
  text-align: center;
  line-height: 88rpx;
  font-size: 32rpx;
  color: #fff;
  position: fixed;
  bottom: 15rpx;
  left: 25rpx;
}