/* pages/my_page/cellphone/cellphone.wxss */
page {
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  padding-top: 50rpx;
}
.clear:after {
  content: '';
  width: 0;
  height: 0;
  display: block;
  clear: both;
  overflow: hidden;
}

.input_box{
  width: 702rpx;
  height: 100rpx;
  background: #f5f5f5;
  margin: 0 auto 35rpx;
  display: flex;
  justify-content:space-between;
  align-items: center;
}
.input_box>input{
  margin-left: 35rpx;
  height: 100rpx;
  width: 60%;
  line-height: 100rpx;
  font-size: 32rpx;
  color: #999;
}
.input_box>view{
  height: 100rpx;
  width: 40%;
}
.input_box>view>view{
  color: #2476E3;
  font-size: 30rpx; 
  border: 1rpx solid #2476E3;
  width: 186rpx;
  height: 66rpx;
  float: right;
  margin-right: 28rpx;
  margin-top: 17rpx;
  text-align: center;
  line-height: 66rpx;
  border-radius: 12rpx;
}
.xyb{
  width: 702rpx;
  height: 100rpx;
  text-align: center;
  line-height: 100rpx;
  background: #FF9913;
  font-size: 32rpx;
  color: #fff;
  margin: 51rpx auto 0;
  font-weight: bold;
  font-family:PingFang-SC-Bold;
  border-radius: 12rpx;
}