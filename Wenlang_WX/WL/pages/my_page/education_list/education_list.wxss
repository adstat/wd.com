/* pages/my_page/education_list/education_list.wxss */
page {
  width: 100%;
  background: #f5f5f5;
  height: 100%;
  box-sizing: border-box;
  padding-top: 25rpx;
}
.clear:after {
  content: '';
  width: 0;
  height: 0;
  display: block;
  clear: both;
  overflow: hidden;
}


.why{
  width: 100%;
  background: #fff;
}
.list{
  width: 100%;
  height: 180rpx;
  display: flex;
  justify-content:space-between;
  align-items: center;
  box-sizing: border-box;
  padding-left: 45rpx;
  padding-right: 45rpx;
  border-bottom: 1rpx solid #e9e9e9;
}
.list>image{
  width: 35rpx;
  height: 35rpx;
}
.list_x{
  color: #2577E3;
  font-size: 32rpx;
  font-weight: bold;
}
.list_time{
  font-size: 22rpx;
  color: #999;
  padding: 10rpx 0;
}
.list_zhuany{
  font-size: 24rpx;
  color: #333;
}
.tianjia{
  background: #fff;
  width: 100%;
  height: 110rpx;
  display: flex;
  align-items: center;
  padding-left: 44rpx;
  box-sizing: border-box;
}
.tianjia>text{
  font-size: 32rpx;
  color: #333;
}
.tianjia>view{
  width: 36rpx;
  height: 36rpx;
  border-radius: 18rpx;
  background: #2577E3;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content:center;
  color: #fff;
  margin-right: 33rpx;
}
.bc{
  width: 702rpx;
  height: 88rpx;
  border-radius: 44rpx;
  background: linear-gradient(to left, #2677E2 , #409AFF);
  text-align: center;
  line-height: 88rpx;
  font-size: 32rpx;
  color: #fff;
  position: fixed;
  bottom: 15rpx;
  left: 25rpx;
}